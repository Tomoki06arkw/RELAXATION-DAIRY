<h2>住所情報登録</h2>

<%= form_for @private_person do |f| %>
  <%= render "devise/shared/error_messages", resource: @private_person %>

  <div class="field">
    <%= f.label :nickname %><br />
    <%= f.text_field :nickname %>
  </div>

  <div class="field">
    <%= f.label :address_id %><br />
    <%= f.text_field :address_id %>
  </div>

  <div class="field">
    <%= f.label :profile %><br />
    <%= f.text_field :profile %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>