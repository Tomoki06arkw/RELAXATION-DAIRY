<h2>住所情報登録</h2>

<%= form_for @private_person do |f| %>
<div class="all">
      <%= render "devise/shared/error_messages", resource: @private_person %>

      <div class="field">
        <%= f.label :nickname %><br />
        <%= f.text_field :nickname, class:"field-main" %>
      </div>

      <div class="field">
        <%= f.label :address %><br />
        <%= f.text_field :address, class: "field-main" %>
      </div>

      <div class="field">
        <%= f.label :profile %><br />
        <%= f.text_field :profile, class: "field-main" %>
      </div>

      <div class="actions">
        <%= f.submit "登録する", class:"submit-btn" %>
      </div>
    <% end %>

    <%= render "devise/shared/links" %>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
    <br>
</div>